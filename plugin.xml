        <config-file target="*ShareSDKParams.plist" parent="params">
            <dict>
              <key>SDK_KEY</key>
              <string>$SDK_KEY</string>
              <key>WeChat</key>
              <dict>
                <key>WECHAT_ID</key>
                <string>$WECHAT_ID</string>
