		<config-file target="assets/ShareSDK.xml" parent="/DevInfor">
            <!-- <meta-data android:name="JPUSH_APPKEY" android:value="$API_KEY"/> -->
            <ShareSDK 
        		AppKey = "$SDK_KEY"/>

			<SinaWeibo
		        Id="1"
		        SortId="1"
		        AppKey="$SINAWEIBO_ID"
		        AppSecret="$SINAWEIBO_SECRET"
		        RedirectUrl="$SINAWEIBO_REDIRECTURI"
		        ShareByAppClient="true"
		        Enable="true" />
		    <Wechat
		        Id="4"
		        SortId="4"
		        AppId="$WECHAT_ID"
		        AppSecret="$WECHAT_SECRET"
		        BypassApproval="false"
		        Enable="true" />

		    <WechatMoments
		        Id="5"
		        SortId="5"
		        AppId="$WECHAT_ID"
		        AppSecret="$WECHAT_SECRET"
		        BypassApproval="false"
		        Enable="true" />
		        
			<WechatFavorite
		        Id="6"
		        SortId="6"
		        AppId="$WECHAT_ID"
		        AppSecret="$WECHAT_SECRET"
		        Enable="true" />
        </config-file>
	</platform>
	<platform name="ios">
